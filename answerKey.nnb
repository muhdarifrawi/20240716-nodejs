            "language": "typescript",
            "source": [
                "let number = 9;\n\nif(number%2 == 0 && number%6 == 0){\n    console.log(\"roll poll\");\n}\nelse if(number%2 == 0){\n    console.log(\"roll\");\n}\nelse if(number%6 == 0){\n    console.log(\"poll\");\n}\nelse{\n    console.log(number);\n}"
